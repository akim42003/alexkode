{
    "function": "solution",
    "test_cases": [
        {
            "name": "converges on x^2",
            "input": {"x_init": 5.0, "lr": 0.1, "steps": 200},
            "expected": {"converged_near_zero": true}
        },
        {
            "name": "2D ill-conditioned",
            "input": {"x_init": [5.0, 5.0], "lr": 0.01, "steps": 500, "function": "x^2+100y^2"},
            "expected": {"converged_near_zero": true}
        },
        {
            "name": "bias correction first step",
            "input": {"x_init": 10.0, "lr": 0.1, "beta1": 0.9, "beta2": 0.999, "steps": 1},
            "expected": {"moved_from_init": true}
        },
        {
            "name": "default hyperparameters",
            "input": {"x_init": 3.0, "lr": 0.001, "beta1": 0.9, "beta2": 0.999, "steps": 1000},
            "expected": {"converged_near_zero": true}
        }
    ]
}
