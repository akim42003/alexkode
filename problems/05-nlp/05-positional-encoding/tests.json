        {
            "name": "values in range -1 to 1",
            "input": {"seq_len": 10, "d_model": 8},
            "expected": {"min_ge": -1.0, "max_le": 1.0, "shape": [10, 8]}
        },
        {
            "name": "even dims are sin odd are cos",
            "input": {"seq_len": 2, "d_model": 2},
            "expected": [[0.0, 1.0], [0.8414709848, 0.5403023059]]
        },
        {
            "name": "large sequence shape",
            "input": {"seq_len": 100, "d_model": 64},
            "expected": {"shape": [100, 64]}
        }
    ]
}
