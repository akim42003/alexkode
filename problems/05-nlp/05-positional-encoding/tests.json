{
    "function": "solution",
    "test_cases": [
        {
            "name": "shape 4 positions 6 dims",
            "input": {"seq_len": 4, "d_model": 6},
            "expected": {"shape": [4, 6]}
        },
        {
            "name": "first position starts with 0",
            "input": {"seq_len": 1, "d_model": 4},
