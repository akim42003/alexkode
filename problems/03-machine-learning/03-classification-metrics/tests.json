{
    "function": "solution",
    "test_cases": [
        {
            "name": "perfect predictions",
            "input": {"y_true": [1, 0, 1, 0, 1], "y_pred": [1, 0, 1, 0, 1]},
            "expected": {"accuracy": 1.0, "precision": 1.0, "recall": 1.0, "f1": 1.0}
        },
        {
            "name": "all wrong",
            "input": {"y_true": [1, 1, 0, 0], "y_pred": [0, 0, 1, 1]},
            "expected": {"accuracy": 0.0, "precision": 0.0, "recall": 0.0, "f1": 0.0}
        },
        {
            "name": "some correct",
            "input": {"y_true": [1, 0, 1, 0, 1, 0], "y_pred": [1, 0, 0, 0, 1, 1]},
            "expected": {"accuracy": 0.6666666667, "precision": 0.6666666667, "recall": 0.6666666667, "f1": 0.6666666667}
        },
        {
            "name": "all positive predictions",
            "input": {"y_true": [1, 0, 1, 0], "y_pred": [1, 1, 1, 1]},
            "expected": {"accuracy": 0.5, "precision": 0.5, "recall": 1.0, "f1": 0.6666666667}
        },
        {
            "name": "all negative predictions",
            "input": {"y_true": [1, 0, 1, 0], "y_pred": [0, 0, 0, 0]},
            "expected": {"accuracy": 0.5, "precision": 0.0, "recall": 0.0, "f1": 0.0}
        }
    ]
}
