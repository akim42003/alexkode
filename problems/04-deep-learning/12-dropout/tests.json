{
    "function": "solution",
    "test_cases": [
        {
            "name": "p=0 no dropout",
            "input": {"x": [[1, 2, 3], [4, 5, 6]], "p": 0.0, "training": true},
            "expected": [[1, 2, 3], [4, 5, 6]]
        },
        {
            "name": "p=1 drop everything",
            "input": {"x": [[1, 2, 3], [4, 5, 6]], "p": 1.0, "training": true},
            "expected": [[0, 0, 0], [0, 0, 0]]
        },
        {
            "name": "inference mode unchanged",
            "input": {"x": [[1, 2, 3], [4, 5, 6]], "p": 0.5, "training": false},
            "expected": [[1, 2, 3], [4, 5, 6]]
        },
        {
            "name": "training mode has zeros",
            "input": {"x": [[1, 2, 3, 4, 5, 6, 7, 8]], "p": 0.5, "training": true, "seed": 42},
            "expected": {"has_zeros": true, "shape": [1, 8]}
        },
        {
            "name": "p=0.5 expected value preserved",
            "input": {"x": [[10, 10, 10, 10, 10, 10, 10, 10, 10, 10]], "p": 0.5, "training": true, "seed": 42},
            "expected": {"mean_approximately": 10.0}
        }
    ]
}
